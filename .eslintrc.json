{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["error", "always"],
        "curly": ["error", "multi-or-nest"]
    }
}
